   <xs:complexType name="IsFollowerInTitleOf">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a RightShare, of which is another RightShare is the follower in title.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="BaseRightShare">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Reference for the BaseRightShare (specific to this message). This is a LocalRightShareAnchorReference starting with the letter S.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:IDREF">
                  <xs:pattern value="S[_0-9a-zA-Z-]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:choice>
            <xs:element name="RightShareUnknown" type="xs:boolean">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The Flag indicating whether the cumulated RightSharePercentage is unknown (=true) or not (=false).</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="RightSharePercentage" type="mc-notif:Percentage">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">The cumulated percentage of rights in the Work that are delegated. This information is given as a xs:decimal value with up to 6 digits after the decimal point (e.g. '12.5' represents 12.5%).</xs:documentation>
                  <xs:documentation source="ddex:Comment">Further Reading: https://kb.ddex.net/implementing-each-standard/best-practices-for-all-ddex-standards/general-guidance-on-messages/communication-of-percentages</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="ManuscriptShare" type="mc-notif:ManuscriptShare">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a MusicalWorkManuscriptShare that is affected by the rights delegation.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
