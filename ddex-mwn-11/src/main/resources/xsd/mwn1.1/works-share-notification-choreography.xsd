               </xs:annotation>
            </xs:element>
            <xs:element name="logo" minOccurs="0" type="mc-notif:Logo">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a logo.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="rights" minOccurs="0" type="mc-notif:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of rights.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="subtitle" minOccurs="0" type="mc-notif:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a subtitle.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="title" type="mc-notif:Text">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a title.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="updated" type="mc-notif:DateTime">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an update.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="entry" minOccurs="0" maxOccurs="unbounded" type="mc-notif:Entry">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of an entry.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:any namespace="##other" minOccurs="0" maxOccurs="unbounded"/>
         </xs:choice>
         <xs:anyAttribute namespace="##other"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Entry">
