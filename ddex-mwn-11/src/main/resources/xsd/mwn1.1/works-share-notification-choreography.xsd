                  <xs:documentation source="ddex:Definition">A Composite containing details of the Name, Identifier and Role(s) of a contributor to a Resource.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="generator" minOccurs="0" type="mc-notif:Generator">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a generator.</xs:documentation>
